{
  "dxt_version": "0.2",
  "name": "@formbricks/hub-mcp",
  "version": "0.3.0",
  "description": "The official MCP Server for the Formbricks Hub API",
  "author": {
    "name": "Formbricks Hub",
    "email": "hola@formbricks.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/formbricks/hub-typescript.git"
  },
  "homepage": "https://github.com/formbricks/hub-typescript/tree/main/packages/mcp-server#readme",
  "documentation": "https://formbricks.com",
  "server": {
    "type": "node",
    "entry_point": "index.js",
    "mcp_config": {
      "command": "node",
      "args": ["${__dirname}/index.js"],
      "env": {
        "HUB_API_KEY": "${user_config.HUB_API_KEY}"
      }
    }
  },
  "user_config": {
    "HUB_API_KEY": {
      "title": "api_key",
      "description": "API key authentication via Bearer token in Authorization header",
      "required": true,
      "type": "string"
    }
  },
  "tools": [],
  "tools_generated": true,
  "compatibility": {
    "runtimes": {
      "node": ">=18.0.0"
    }
  },
  "keywords": ["api"]
}
